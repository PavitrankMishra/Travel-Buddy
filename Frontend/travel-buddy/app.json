{
  "expo": {
    "name": "travel-buddy",
    "slug": "travel-buddy",
    "version": "1.0.0",
    "scheme": "travel-buddy",
    "orientation": "portrait",
    "icon": "./src/assets/Logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/Logo.png",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/Logo.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.pavitrank.travelbuddy"
    },
    "web": {
      "favicon": "./src/assets/Logo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fcbe4aad-6ec4-4de7-ae16-ab0bc60a002c"
      }
    }
  }
}
