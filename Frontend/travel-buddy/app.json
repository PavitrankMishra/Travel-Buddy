{
  "expo": {
    "name": "travel-buddy",
    "slug": "travel-buddy",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "travelbuddy",
    "userInterfaceStyle": "automatic",
    "entryPoint": "expo-router/entry",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      ["expo-splash-screen"],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
