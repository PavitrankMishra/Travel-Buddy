{
  "expo": {
    "name": "travel-buddy",
    "slug": "travel-buddy",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "travelbuddy",
    "userInterfaceStyle": "automatic",
    "entryPoint": "expo-router/entry",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pavitrank.travelbuddy",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.pavitrank.travelbuddy"
    },
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen"
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fcbe4aad-6ec4-4de7-ae16-ab0bc60a002c"
      }
    }
  }
}
