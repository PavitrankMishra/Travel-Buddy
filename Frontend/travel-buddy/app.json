{
  "expo": {
    "name": "travel-buddy",
    "slug": "travel-buddy",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "travelbuddy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static"
    },
    "plugins": ["expo-router", ["expo-splash-screen"]],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
