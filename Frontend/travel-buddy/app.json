{
  "expo": {
    "name": "travel-buddy",
    "slug": "travel-buddy",
    "version": "1.0.0",
    "scheme": "travel-buddy",
    "orientation": "portrait",
    "icon": "./src/assets/Logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/Logo.png",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/Logo.png"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./src/assets/Logo.png"
    },
    "plugins": ["expo-router"]
  }
}
